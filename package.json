    "@nx/esbuild": "16.1.1",
    "@nx/eslint-plugin": "16.1.1",
    "@nx/jest": "16.1.1",
    "@nx/js": "16.1.1",
    "@nx/linter": "16.1.1",
    "@nx/node": "16.1.1",
    "@nx/react": "16.1.1",
