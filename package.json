    "@nx/next": "^16.1.1",
    "aws-lambda": "^1.0.7",
    "axios": "^1.0.0",
    "mongodb": "^5.6.0",
    "next": "13.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
