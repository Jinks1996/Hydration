{
  "name": "hydration",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "nx run services:build:production --skip-nx-cache",
    "build:test": "nx run services:build:test --skip-nx-cache",
    "offline": "cd apps/services && serverless offline --stage local",
    "preservices:deploy": "cp package.json apps/services/package.json",
    "services:deploy": "cd apps/services && serverless package",
    "postservices:deploy": "rm -f apps/services/package.json && rm -fr apps/services/node_modules",
    "preservices:deploy:test": "cp package.json apps/services/package.json",
    "services:deploy:test": "cd apps/services && serverless package --stage test",
    "postservices:deploy:test": "rm -f apps/services/package.json && rm -fr apps/services/node_modules",
    "preservices:deploy:dev": "cp package.json apps/services/package.json",
    "services:deploy:dev": "cd apps/services && serverless package --stage dev",
    "postservices:deploy:dev": "rm -f apps/services/package.json && rm -fr apps/services/node_modules"
  },
  "private": true,
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.338.0",
